{
    "git.autofetch": true,

    "editor.tabSize": 2,
    "editor.rulers": [120],
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.exclude": {
      "**/.git": true,
      "**/.DS_Store": true
    },
    "eslint.enable": false,
    "tslint.autoFixOnSave": true,
    "typescript.format.enable": true,
    "typescript.tsdk": "node_modules/typescript/lib"
  }
  